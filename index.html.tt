<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html><head>
<meta http-equiv="Content-Type" content="text/html;charset=utf-8">
<title>Startup Links for Ryan D Johnson</title>
<style type="text/css">
[%+ INCLUDE index.css.tt +%]
</style>
</head><body>
[% FOREACH column = link_columns %]
  <div class="c">
    [% FOREACH link_group IN column %]
      <div class="g"><div class="l">[% link_group.group_name %]</div><ul>
        [% FOREACH link IN link_group.links %]
          <li><a href="[% link.uri %]">[% link.name %]</a></li>
        [% END %]
      </ul></div>
    [% END %]
  </div>
[% END %]
<input type="text" id="focus_me">
<script type="text/javascript">
( function () {
    focus_me = document.getElementById('focus_me');
    focus_me.focus();
    focus_me.blur();
    focus_me.parentNode.removeChild( focus_me );
} )();
</script>
</body></html>
